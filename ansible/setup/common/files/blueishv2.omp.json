{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#546E7A",
          "foreground": "#ADA9A8",
          "leading_diamond": "\ue0b6",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "properties": {
            "wsl": "",
            "wsl_separator": ""
          },
          "style": "diamond",
          "type": "os"
        },
        {
          "background": "#7aadf4",
          "foreground": "#546E7A",
          "powerline_symbol": "\ue0b0",
          "template": " {{ if .SSHSession }} \uD83D\uDCBB {{ end }} {{.UserName}} {{.HostName}}@{{.Env.TLS_SAN}} ",
          "background_templates": [
            "{{if (ne \"cloud.private\" .Env.TLS_SAN)}}#741903{{end}}"
          ],
          "style": "powerline",
          "type": "session"
        },
        {
          "background": "#546E7A",
          "foreground": "#ADA9A8",
          "powerline_symbol": "\ue0b0",
          "template": " \ue5ff <transparent>\ue0bd </>{{ .Path }} <transparent>\ue0bd </>",
          "properties": {
            "folder_separator_icon": "<transparent> \ue0bd </>",
            "home_icon": "\uf7dd",
            "mixed_threshold": 10,
            "style": "folder"
          },
          "style": "powerline",
          "type": "path"
        },
        {
          "background": "#fffb38",
          "foreground": "#546E7A",
          "powerline_symbol": "\ue0b0",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ],
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "git_icon": "\uf09b"
          },
          "style": "powerline",
          "type": "git"
        },
        {
          "background": "#6CA35E",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "style": "powerline",
          "type": "node"
        },
        {
          "background": "#ffff66",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "template": " \uf0e7 ",
          "style": "powerline",
          "type": "root"
        },
        {
          "type": "go",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#7FD5EA",
          "template": " \uFCD1 {{ .Full }} "
        },
        {
          "background": "#0077c2",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "template": " \ufd31 {{.Context}} :: {{if .Namespace}}{{.Namespace}}{{else}}default{{end}} ",
          "style": "powerline",
          "type": "kubectl"
        },
        {
          "background": "#546E7A",
          "foreground": "#ADA9A8",
          "powerline_symbol": "\ue0b0",
          "template": " <#fefefe>\ufbab</> {{ .FormattedMs }} ",
          "properties": {
            "style": "austin",
            "threshold": 1
          },
          "style": "powerline",
          "type": "executiontime"
        },
        {
          "background": "#007800",
          "background_templates": [
            "{{ if gt .Code 0 }}#f1184c{{ end }}"
          ],
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b4",
          "template": " \ufc8d ",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#007ACC",
          "style": "plain",
          "template": "\u279c ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "{{ .Shell }}> "
  },
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{if .Root}} \u26a1 {{end}}{{.Folder | replace \"~\" \"üè†\"}} @ {{.HostName}}",
  "final_space": true,
  "version": 2
}